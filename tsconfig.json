{
  "compilerOptions": {
    "target": "es6",
    "module": "ES6",
    "rootDir": "./",
    "moduleResolution": "node",
    "noEmit": true,
    "sourceRoot": "./src",
    "inlineSourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}